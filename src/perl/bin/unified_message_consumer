#! /usr/bin/env perl

use strict;
use warnings;
use FindBin qw($Bin);
use lib ( -d "$Bin/../lib/perl5" ? "$Bin/../lib/perl5" : "$Bin/../lib" );

use wtsi_clarity::mq::mh::warehouse_message_handler;

require qw{consumer};

my $message_handler = wtsi_clarity::mq::mh::warehouse_message_handler->new();

start_consumer('Unified Warehouse Message Consumer', 'warehouse', $message_handler);

1;